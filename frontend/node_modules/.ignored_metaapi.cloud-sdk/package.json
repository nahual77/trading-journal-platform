    "@swc-node/register": "~1.8.0",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.56",
    "@swc/types": "0.1.6",
    "@types/mocha": "~10.0.1",
    "@types/sinon": "^10.0.19",
    "@typescript-eslint/eslint-plugin": "~5.62.0",
    "@typescript-eslint/parser": "~5.62.0",
    "assert": "~2.0.0",
    "copyfiles": "~2.4.1",
    "cross-env": "~7.0.3",
    "dotenv": "~16.0.3",
    "eslint": "~7.1.0",
    "eslint-plugin-mocha": "~7.0.0",
    "fake-indexeddb": "~3.1.2",
    "log4js": "~6.9.1",
    "mocha": "~7.2.0",
    "module-alias": "~2.2.2",
    "should": "~13.2.3",
    "sinon": "~9.0.2",
    "socket.io": "~2.4.0",
    "swc-loader": "~0.2.3",
    "terser-webpack-plugin": "~5.3.9",
    "typescript": "~5.0.4",
    "webpack": "~5.82",
    "webpack-cli": "~5.0.2"
  },
  "browser": {
    "path": false,
    "fs": false,
    "os": false,
    "tls": false
  },
  "scripts": {
    "test": "cross-env SWCRC=true mocha --exit --require @swc-node/register --require ./aliases.js 'lib/**/*.spec.*'",
    "lint:fix": "eslint --ext es6 --ext ts ./lib --fix",
    "lint": "eslint --ext es6 --ext ts ./lib",
    "build-win": "npm run clean-win | npm run build:optimized && npm run build:transpile && npm run build:types && npm run build:mv-manual-types && npm run build:mv-compiled-types && npm run build:mv-mjs-types && npm run build:backward-compatiibility",
    "build": "npm run clean; npm run build:optimized && npm run build:transpile && npm run build:types && npm run build:mv-manual-types && npm run build:mv-compiled-types && npm run build:mv-mjs-types && npm run build:backward-compatiibility",
    "build:optimized": "npx webpack --config webpack.config.js",
    "build:transpile": "node ./swc/swc.transform.cjs.js && node ./swc/swc.transform.esm.js ",
    "build:types": "npx tsc --emitDeclarationOnly --outDir .temp/types",
    "build:mv-compiled-types": "copyfiles -u 2 \".temp/types/**/*.d.ts\" build/esm && copyfiles -u 2 \".temp/types/**/*.d.ts\" build/cjs && copyfiles -u 2 \".temp/types/**/*.d.ts\" dists/cjs && copyfiles -u 2 \".temp/types/**/*.d.ts\" dists/esm-web && copyfiles -u 2 \".temp/types/**/*.d.ts\" dists/esm-node && copyfiles -u 2 \".temp/types/**/*.d.ts\" dists/umd",
    "build:mv-manual-types": "copyfiles -u 1 \"lib/**/*.d.ts\" build/esm && copyfiles -u 1 \"lib/**/*.d.ts\" build/cjs && copyfiles -u 1 \"lib/**/*.d.ts\" dists/cjs && copyfiles -u 1 \"lib/**/*.d.ts\" dists/esm-web && copyfiles -u 1 \"lib/**/*.d.ts\" dists/esm-node && copyfiles -u 1 \"lib/**/*.d.ts\" dists/umd",
    "build:mv-mjs-types": "mv dists/esm-node/index.d.ts dists/esm-node/index.d.mts",
    "build:backward-compatiibility": "copyfiles -u 2 \"build/cjs/**/*\" dist/",
    "clean-win": "(if exist dist rmdir /Q /S dist) && (if exist build rmdir /Q /S build) && (if exist dists rmdir /Q /S dists) && (if exist .temp\\types rmdir /Q /S .temp\\types)",
    "clean": "rm -rf build; rm -rf dist; rm -rf dists; rm -rf .temp/types",
    "type-check": "tsc --noEmit --emitDeclarationOnly false",
    "tc": "npm run type-check"
  },
  "module": "./dists/esm-web/index.js",
  "unpkg": "./dists/umd/index.js",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dists/esm-web/index.js",
      "require": "./dist/index.js"
    },
    "./dist/*": "./dist/*.js",
    "./dist/": "./dist/",
    "./cjs": "./build/cjs/index.js",
    "./esm": "./build/esm/index.js",
    "./esm-node": "./dists/esm-node/index.mjs",
    "./browser": "./dists/umd/index.js",
    "./node": "./dists/cjs/index.js",
    "./web": "./dists/esm-web/index.js"
  }
}
