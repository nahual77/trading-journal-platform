#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Diario/Trading-Journal/frontend/node_modules/.pnpm/randomstring@1.2.3/node_modules/randomstring/bin/node_modules:/mnt/d/Diario/Trading-Journal/frontend/node_modules/.pnpm/randomstring@1.2.3/node_modules/randomstring/node_modules:/mnt/d/Diario/Trading-Journal/frontend/node_modules/.pnpm/randomstring@1.2.3/node_modules:/mnt/d/Diario/Trading-Journal/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Diario/Trading-Journal/frontend/node_modules/.pnpm/randomstring@1.2.3/node_modules/randomstring/bin/node_modules:/mnt/d/Diario/Trading-Journal/frontend/node_modules/.pnpm/randomstring@1.2.3/node_modules/randomstring/node_modules:/mnt/d/Diario/Trading-Journal/frontend/node_modules/.pnpm/randomstring@1.2.3/node_modules:/mnt/d/Diario/Trading-Journal/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../randomstring@1.2.3/node_modules/randomstring/bin/randomstring" "$@"
else
  exec node  "$basedir/../../../../../randomstring@1.2.3/node_modules/randomstring/bin/randomstring" "$@"
fi
